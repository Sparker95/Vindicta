    "version": "2.0.0",
    "tasks": [
        {
            "label": "Compile with SQF-VM",
            "type": "process",
            "command": "../tools/SQF-VM/sqfvm.exe",
            "args": [
                "-a",
                "--no-execute-print",
                "--parse-only",
                "--load",
                ".",
                "-i",
                "${file}",
                "--disable-macro-warnings"
            ],
            "options": {
                "cwd": "${workspaceRoot}/Project_0.Altis"
            },
            "group": "build",
            "presentation": {
                "reveal": "always",
                "clear": true,
                "focus": true,
                "group": "SQF-VM"
            },
            "problemMatcher": [
                {
                    "owner": "sqf",
                    "fileLocation": "absolute",
                    "pattern": [
                        {
                            "regexp": "\\[(RNT|WRN|ERR)\\]\\[L?(.+)\\|C(.+)\\|([a-zA-Z]\\:.*?)\\](.*)",
                            "line": 2,
                            "column": 3,
                            "severity": 1,
                            "file": 4,
                            "message": 5
                        }
                    ]
                },
                {
                    "owner": "sqf",
                    "fileLocation": [
                        "relative",
                        "${workspaceRoot}/Project_0.Altis"
                    ],
                    "pattern": [
                        {
                            "regexp": "\\[(RNT|WRN|ERR)\\]\\[L?(.+)\\|C(.+)\\|(.[^\\:].*?)\\](.*)",
                            "line": 2,
                            "column": 3,
                            "severity": 1,
                            "file": 4,
                            "message": 5
                        }
                    ]
                }
            ]
        },
